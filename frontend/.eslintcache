n    history.push(`/cart/${match.params.id}?qty=${qty}`)\n  }\n\n  const submitHandler = (e) => {\n    e.preventDefault()\n    dispatch(\n      createProductReview(match.params.id, {\n        rating,\n        comment,\n      })\n    )\n    setComment('')\n    setRating(0)\n  }\n\n  return (\n    <>\n      <ButtonBack linkTo='/' />\n\n      <div className='center'>\n        {loading ? (\n          <Loader />\n        ) : error ? (\n          <Message type='error' message='Error' description={error} />\n        ) : (\n          <Row gutter={[12, 12]}>\n            <Col\n              lg={{span: 8}}\n              md={{span: 12}}\n              xs={{span: 24}}\n              className='image-product'>\n              <Card\n                className='image-card'\n                bordered\n                cover={<img alt={product.name} src={product.image} />}\n              />\n            </Col>\n            <Col\n              lg={{span: 12}}\n              md={{span: 12}}\n              xs={{span: 24}}\n              className='info-product'>\n              <List itemLayout='horizontal' size='large' bordered>\n                <List.Item>\n                  <h3>{product.name}</h3>\n                </List.Item>\n\n                <List.Item>\n                  <Rating\n                    value={product.rating}\n                    text={`${product.numReviews} reviews`}\n                  />\n                </List.Item>\n                <List.Item>\n                  <strong>Price:</strong> ${product.price}\n                </List.Item>\n                <List.Item>\n                  <strong>Description:</strong> {product.description}\n                </List.Item>\n              </List>\n            </Col>\n\n            <Col lg={{span: 4}} md={{span: 12}} xs={{span: 24}}>\n              <List bordered>\n                <List.Item>\n                  <strong>Status: </strong>\n                  {product.countInStock > 0 ? 'In Stock' : 'Out Of Stock'}\n                </List.Item>\n\n                <List.Item>\n                  <strong>Brand: </strong>\n                  {product.brand}\n                </List.Item>\n              </List>\n\n              {product.countInStock > 0 && (\n                <Button\n                  type='primary'\n                  size='large'\n                  block\n                  disabled={product.countInStock === 0}\n                  onClick={addToCartHandler}>\n                  Add To Cart\n                </Button>\n              )}\n            </Col>\n          </Row>\n        )}\n\n        <Divider />\n\n        <Row gutter={[24, 36]}>\n          <Col lg={{span: 12}} md={{span: 12}} xs={{span: 24}}>\n            <h2>Reviews</h2>\n            {product.reviews.length === 0 && (\n              <Message description='No Reviews' />\n            )}\n\n            {product.reviews.map((review) => (\n              <div key={review._id}>\n                <strong>{review.name}</strong>\n                <Rating value={review.rating} />\n                <p>{review.createdAt.substring(0, 10)}</p>\n                <p>{review.comment}</p>\n              </div>\n            ))}\n          </Col>\n\n          <Col lg={{span: 12}} md={{span: 12}} xs={{span: 24}}>\n            <h2>Write a Customer Review</h2>\n            {successProductReview && (\n              <Message\n                type='success'\n                description='Review submitted successfully'\n              />\n            )}\n            {loadingProductReview && <Loader />}\n            {errorProductReview && (\n              <Message type='danger' description={errorProductReview} />\n            )}\n            {userInfo ? (\n              <Form layout='vertical'>\n                <Form.Item>\n                  <Select\n                    defaultValue=''\n                    style={{width: 120}}\n                    onChange={(value) => setRating(value)}>\n                    <Option value=''>Select...</Option>\n                    <Option value='5'>5 - Excellent</Option>\n                    <Option value='4'>4 - Very Good</Option>\n                    <Option value='3'>3 - Good</Option>\n                    <Option value='2'>2 - Fair</Option>\n                    <Option value='1'>1 - Poor</Option>\n                  </Select>\n                </Form.Item>\n\n                <Form.Item\n                  name={['user', 'comment']}\n                  onChange={(e) => setComment(e.target.value)}\n                  label='Comment'>\n                  <Input.TextArea />\n                </Form.Item>\n\n                <Form.Item>\n                  <Button\n                    type='primary'\n                    htmlType='submit'\n                    onClick={submitHandler}>\n                    Submit\n                  </Button>\n                </Form.Item>\n              </Form>\n            ) : (\n              <>\n                <Message description='Please Sign in to write a review' />\n                <Button>\n                  <Link to='/login'>Sign in</Link>\n                </Button>\n              </>\n            )}\n          </Col>\n        </Row>\n      </div>\n    </>\n  )\n}\n\nexport default ProductScreen\n","/Users/thuynguyen/Documents/minthsShop/frontend/src/screens/LoginScreen/LoginScreen.js",[],"/Users/thuynguyen/Documents/minthsShop/frontend/src/reducers/cartReducer.js",[],["137","138"],"/Users/thuynguyen/Documents/minthsShop/frontend/src/components/CheckoutSteps/CheckoutSteps.js",[],"/Users/thuynguyen/Documents/minthsShop/frontend/src/components/UI Handler/Loader/Loader.js",[],"/Users/thuynguyen/Documents/minthsShop/frontend/src/components/StoryCard/StoryCard.js",[],"/Users/thuynguyen/Documents/minthsShop/frontend/src/components/Rating/Rating.js",[],"/Users/thuynguyen/Documents/minthsShop/frontend/src/actions/orderActions.js",[],"/Users/thuynguyen/Documents/minthsShop/frontend/src/actions/productActions.js",[],"/Users/thuynguyen/Documents/minthsShop/frontend/src/s